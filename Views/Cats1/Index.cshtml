@model IEnumerable<trial2.Models.Cats1>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/shared/_MyLayout.cshtml";
}

<h2>قائمة الوظائف</h2>

<p>
    @Html.ActionLink("أنشاءنوع جديد", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.catname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.catdescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.catname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.catdescription)
        </td>
        <td>
            @Html.ActionLink("تعديل", "Edit", new { id=item.Id }) |
            @Html.ActionLink("تفاصيل", "Details", new { id=item.Id }) |
            @Html.ActionLink("حذف", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
